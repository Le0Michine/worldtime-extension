{
  "name": "worldtime",
  "version": "1.5.0",
  "description": "Several timezones in popup",
  "type": "module",
  "repository": "https://github.com/Le0Michine/timezone-extension",
  "author": "Lev Mishin",
  "license": "ISC",
  "scripts": {
    "test": "vitest",
    "test:watch": "vitest --watch --ui --coverage",
    "dev": "vite",
    "build": "npm run build:clean && npm run build:app && npm run build:options && npm run build:copy",
    "build:clean": "rm -fr ./dist",
    "build:app": "vite build",
    "build:options": "vite build --config vite.options-config.mts",
    "build:copy": "cp -R ./src/icons_prod ./dist/icons && cp ./src/manifest.json ./dist/manifest.json",
    "build:prod": "webpack --config config/webpack.prod.js",
    "build:canary": "webpack --config config/webpack.canary.js -p",
    "build:firefox": "export TARGET=firefox && webpack --config config/webpack.prod.js -p",
    "build:demo": "export TARGET=demo && webpack --config config/webpack.prod.js -p"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@fontsource/roboto": "^5.2.0",
    "@mui/icons-material": "^6.4.6",
    "@mui/material": "^6.4.6",
    "@mui/styles": "^6.4.7",
    "@reduxjs/toolkit": "^2.6.1",
    "@types/react-autosuggest": "^10.1.11",
    "autosuggest-highlight": "^3.3.4",
    "bootstrap": "^5.3.3",
    "d3": "^7.9.0",
    "d3-geo": "^3.1.1",
    "d3-geo-projection": "^4.0.0",
    "diacritic": "^0.0.2",
    "history": "^5.3.0",
    "lodash-es": "^4.17.21",
    "moment": "^2.30.1",
    "moment-timezone": "^0.5.47",
    "react": "^19.0.0",
    "react-autosuggest": "^10.1.0",
    "react-dom": "^19.0.0",
    "react-redux": "^9.2.0",
    "react-router": "^7.2.0",
    "react-router-dom": "^7.2.0",
    "react-router-redux": "^4.0.8",
    "redux": "^5.0.1",
    "redux-localstorage": "^0.4.1",
    "redux-logger": "^3.0.6",
    "topojson": "^3.0.2",
    "tss-react": "^4.9.15"
  },
  "devDependencies": {
    "@types/chrome": "^0.0.307",
    "@types/d3": "^7.4.3",
    "@types/d3-geo": "^3.1.0",
    "@types/lodash-es": "^4.17.12",
    "@types/moment-timezone": "^0.5.30",
    "@types/prettier": "^3.0.0",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@types/react-redux": "^7.1.34",
    "@types/react-router": "^5.1.20",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-router-redux": "^5.0.27",
    "@types/redux-localstorage": "^1.0.12",
    "@types/redux-logger": "^3.0.13",
    "@types/topojson": "^3.2.6",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "^3.0.7",
    "@vitest/ui": "^3.0.7",
    "eslint": "^9.21.0",
    "eslint-plugin-react": "^7.37.4",
    "prettier": "^3.5.2",
    "sass-embedded": "^1.85.1",
    "turbo": "^2.4.4",
    "typescript": "^5.8.2",
    "vite": "^6.2.0",
    "vitest": "^3.0.7"
  }
}
