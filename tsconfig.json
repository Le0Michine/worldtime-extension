{
  "compilerOptions": {
    "jsx": "react-jsx",
    "target": "ESNext",
    "module": "Preserve",
    "moduleResolution": "Bundler",
    "sourceMap": true,
    "removeComments": true,
    "skipLibCheck": true,
    "noImplicitAny": false,
    "lib": ["dom"],
    "types": ["vite/client"]
  },
  "exclude": ["node_modules"],
  "compileOnSave": false,
  "buildOnSave": false
}
